{
  "name": "next-js",
  "version": "1.0.0",
  "main": "index.js",
  "author": "kaave <abe@framelunch.jp>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.server.json server",
    "build": "next build && tsc --project tsconfig.server.json",
    "export": "next export",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "start": "NODE_ENV=production node .build/server",
    "lint": "run-p lint:*",
    "lint:style": "stylelint '**/*.scss'",
    "lint:script": "eslint '**/*.{js,ts,tsx}'",
    "fix": "run-p fix:*",
    "fix:style": "stylelint --fix '**/*.scss'",
    "fix:script": "eslint --fix '**/*.{js,ts,tsx}'",
    "fix:json": "prettier --write '**/*.json'",
    "test": "jest",
    "watch": "jest --watchAll",
    "typecheck": "tsc --noEmit -p tsconfig.server.json"
  },
  "browserslist": [
    "> 0.5% in jp",
    "node >= 10",
    "not ios < 10.3",
    "not android < 5",
    "not safari < 10.1",
    "not chrome 49",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sanitize.css": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/dotenv": "^6.1.1",
    "@types/dotenv-webpack": "^1.7.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/next": "^8.0.5",
    "@types/node": "^10",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "autoprefixer": "^9.6.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "node-sass-package-importer": "^5.3.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-properties": "^8.0.10",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.2",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
