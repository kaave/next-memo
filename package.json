{
  "name": "next-js",
  "version": "1.0.0",
  "main": "index.js",
  "author": "kaave <abe@framelunch.jp>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node --extensions '.ts,.tsx' server/index.ts",
    "build": "run-p build:*",
    "build:client": "next build",
    "build:server": "babel server --extensions '.js,.ts,.tsx' --out-dir .build",
    "export": "next export",
    "sitemap": "node sitemap-generator",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "start": "NODE_ENV=production node .build",
    "lint": "run-p lint:*",
    "lint:style": "stylelint '**/*.scss'",
    "lint:script": "eslint '**/*.{js,ts,tsx}'",
    "fix": "run-p fix:*",
    "fix:style": "stylelint --fix '**/*.scss'",
    "fix:script": "eslint --fix '**/*.{js,ts,tsx}'",
    "fix:json": "prettier --write '**/*.json'",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "lint-staged": "lint-staged"
  },
  "browserslist": [
    "> 0.5% in jp",
    "node >= 10",
    "not ios < 10.3",
    "not android < 5",
    "not safari < 10.1",
    "not chrome 49",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "run-p typecheck lint-staged"
    }
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "immer": "^3.1.3",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.0.1",
    "next-offline": "^4.0.2",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.5",
    "sanitize.css": "^11.0.0",
    "styled-components": "^4.3.2",
    "typesafe-actions": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/dotenv-webpack": "^1.7.0",
    "@types/enzyme": "^3.10.2",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "^0.0.42",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/next": "^8.0.6",
    "@types/next-redux-saga": "^3.0.1",
    "@types/next-redux-wrapper": "^2.0.2",
    "@types/node": "^10",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-redux": "^3.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^9.2.0",
    "nextjs-sitemap-generator": "^0.0.6",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-properties": "^9.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.2",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.1",
    "typescript": "^3.5.3"
  }
}
